{
  "name": "procedure_cost_us",
  "version": "0.0.0",
  "description": "An exploration of hospital procedure costs",
  "homepage": "https://github.com/cdepman/hospital-procedure-heatmap",
  "keywords": [
    "healthcare",
    "heatmap",
    "visualization",
    "gis",
    "leaflet"
  ],
  "author": "Charlie Depman",
  "repository": {
    "type": "git",
    "url": "git@github.com:cdepman/hospital-procedure-heatmap.git"
  },
  "devDependencies": {
    "eslint": "^1.7.3",
    "eslint-config-mourner": "^1.0.1",
    "simpleheat": "~0.2.0",
    "uglify-js": "^2.5.0"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "pug": "^2.0.0-alpha3",
    "serve-favicon": "~2.3.0"
  },
  "eslintConfig": {
    "extends": "mourner",
    "globals": {
      "L": false,
      "simpleheat": false
    }
  },
  "scripts": {
    "test": "eslint public/src",
    "prepublish": "uglifyjs node_modules/simpleheat/simpleheat.js ./public/js/HeatLayer.js -c -m -o ./public/dist/leaflet-heat.js",
    "start": "node ./server/bin/www"
  },
  "license": "BSD-2-Clause",
  "jshintConfig": {
    "quotmark": "single",
    "globals": {
      "L": true,
      "simpleheat": true
    },
    "trailing": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "noempty": true,
    "nonbsp": true,
    "undef": true,
    "unused": true,
    "browser": true
  }
}
